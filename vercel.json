{
  "buildCommand": "npm run build && npm run build-storybook",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook/index.html"
    },
    {
      "source": "/storybook/:path*",
      "destination": "/storybook/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
